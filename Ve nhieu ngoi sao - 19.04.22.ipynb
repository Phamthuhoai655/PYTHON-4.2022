   "source": [
    "import turtle\n",
    "h = turtle.Turtle()\n",
    "h.pensize(1)\n",
    "h.speed(5)\n",
    "N = 5\n",
    "h.penup()\n",
    "h.backward(300)\n",
    "h.pendown()\n",
    "for j in range(0,N):\n",
    "    h.penup()\n",
    "    h.forward(100)\n",
    "    h.pendown()\n",
    "    for i in range(0,5):\n",
    "        h.forward(100)\n",
    "        h.right(144)\n",
    "turtle.done()"
   ]
